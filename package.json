{
  "name": "base-task-alias",
  "version": "0.1.6",
  "description": "Plugin that adds `.taskAlias` method to your @node-base application. Creating alias task for some task.",
  "repository": "tunnckoCore/base-task-alias",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "commit": "git add --all",
    "postcommit": "git-cz"
  },
  "dependencies": {
    "base-task": "^0.7.0",
    "extend-shallow": "^2.0.1",
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "base": "^0.11.1",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "mukla": "^0.4.1",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "alias",
    "aliases",
    "base",
    "basejs",
    "baseplugin",
    "node",
    "plugin",
    "task",
    "tasks"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "base-plugins-enhanced",
        "base",
        "base-plugins",
        "base-task",
        "verb",
        "verb-generate-readme",
        "generate"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
